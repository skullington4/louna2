// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Items {
  id            String  @id @default(auto()) @map("_id") @db.ObjectId
  imageName     String
  title         String
  description   String
}

model Collections {
  id            String  @id @default(auto()) @map("_id") @db.ObjectId
  title         String
}
