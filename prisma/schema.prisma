generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Items {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  description String
  imageName   String
  title       String
  collection  String
}

model Collections {
  id            String  @id @default(auto()) @map("_id") @db.ObjectId
  title         String
  imageName     String
}